#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    vector<int> arr(n);
    for(int i=0;i<n;i++)
    {
    cin>>arr[i];
    }
    int num;
    int count;
    for(int i=0;i<n;i++)
    {
        num=arr[i];
         count=0;
        for(int j=0;j<n;j++)
        {
        if(arr[j]==num)
        {
        count++;
        }
        }
        if(count==1)
        {
            cout<<num<<endl;
            return 0;
        }
    }
    
}
