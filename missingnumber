#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    vector<int> arr(n);
    // int arr[n];
    for(int i=0;i<n;i++)
    {
    cin>>arr[i];
    }
    int sum=(n*(n+1))/2;
    int s2=0;
    for(int i=0;i<n;i++)
    {
        s2+=arr[i];
    }
    int ans=sum-s2;
    cout<<ans;
    return 0;
}
